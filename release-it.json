{
  "git": {
    "commitMessage": "chore: release v${version}",
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "infile": "./CHANGELOG.md",
      "preset": "angular"
    }
  },
  "github": {
    "release": true,
    "tokenRef": "PERSONAL_ACCESS_TOKEN",
    "autoGenerate": true
  },
  "npm": "false"
}
